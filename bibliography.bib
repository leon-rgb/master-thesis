	number = {3},
	urldate = {2024-01-18},
	journal = {European Journal of Technology},
	author = {Aslam, Farhan},
	month = aug,
	year = {2023},
	note = {Number: 3},
	keywords = {Artificial Intelligence, Chatbot Technology, Deep Learning, Machine Learning, Natural Language Processing, Virtual Assistants, Voice-Enabled Chatbots},
	pages = {62--72},
	file = {Full Text PDF:C\:\\Users\\lop1le\\Zotero\\storage\\CCRU96AL\\Aslam - 2023 - The Impact of Artificial Intelligence on Chatbot T.pdf:application/pdf},
